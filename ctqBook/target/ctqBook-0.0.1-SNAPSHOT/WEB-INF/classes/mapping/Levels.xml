<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="jsdx.zr.dao.LevelDao">
	<select id="findLevelById" parameterType="int" resultType="Levels">
		select * from t_levels where level = #{id}
	</select>
	<select id="findAllLevel"  resultType="Levels">
		select * from t_levels 
	</select>
</mapper> 